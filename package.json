{
  "name": "startup-nextjs-template",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "cross-env NODE_ENV=development next dev -p 3030",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "cross-env NODE_ENV=production next start -p 3030",
    "commit": "git-cz",
    "lint": "next lint --fix",
    "lint-fix": "eslint --fix .",
    "release": "standard-version",
    "postbuild": "next-sitemap --config next-sitemap.config.js"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@next/third-parties": "^14.1.4",
    "@reduxjs/toolkit": "1.9.5",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "accept-language": "^3.0.18",
    "axios": "^1.6.7",
    "i18next": "^23.10.0",
    "i18next-resources-to-backend": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.1.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-modal-video": "^2.0.1",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-angular": "^19.1.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.33",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "express": "^4.19.2",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^9.0.11",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}